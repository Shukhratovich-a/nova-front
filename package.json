{
  "name": "nova-front",
  "version": "0.1.13",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build && node copy-assets.js",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "build:uz": "cross-env APP_ENV=uz NEXT_PUBLIC_APP_ENV=uz next build",
    "build:tr": "cross-env APP_ENV=tr NEXT_PUBLIC_APP_ENV=tr next build",
    "build:eg": "cross-env APP_ENV=eg NEXT_PUBLIC_APP_ENV=eg next build",
    "start:uz": "cross-env APP_ENV=uz next start -p 3001",
    "start:tr": "cross-env APP_ENV=tr next start -p 3002",
    "start:eg": "cross-env APP_ENV=eg next start -p 3003",
    "build:all": "pnpm run build:uz && pnpm run build:tr && pnpm run build:eg",
    "start:all": "pnpm run start:uz & pnpm run start:tr & pnpm run start:eg"
  },
  "dependencies": {
    "axios": "1.6.7",
    "classnames": "2.5.1",
    "date-fns": "3.3.1",
    "framer-motion": "11.0.6",
    "i18next": "23.8.2",
    "next": "14.1.0",
    "next-i18next": "15.2.0",
    "query-string": "8.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.0.5",
    "react-image-gallery": "1.4.0",
    "react-responsive": "9.0.2",
    "sass": "1.70.0",
    "sharp": "0.32.6",
    "swiper": "11.0.5"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/react-image-gallery": "1.2.4",
    "cross-env": "^10.1.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "typescript": "5.3.3"
  }
}
